        <CheckBox x:Name="debugChckBox" Content="Debugging" HorizontalAlignment="Left" Margin="10,245,0,0" VerticalAlignment="Top" Unchecked="debugChckBox_Unchecked" Checked="debugChckBox_Checked"/>
        <Button x:Name="SaveBtn" Content="Save" HorizontalAlignment="Left" Margin="415,245,0,0" VerticalAlignment="Top" Width="75" Click="SaveBtn_Click"/>

    </Grid>
